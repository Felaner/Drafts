<div class="container">
    <ul class="project-list list-unstyled f-38">
        {{#if maxFiles}}
            <div class="alert alert-danger text-center">{{maxFiles}}</div>
        {{/if}}
        {{#if editSuccess}}
            <div class="alert alert-success text-center">{{editSuccess}}</div>
        {{/if}}
        {{#if deleteSuccess}}
            <div class="alert alert-success text-center">{{deleteSuccess}}</div>
        {{/if}}
        {{#each projects}}
            <li>
                <a href="/portfolio/edit/{{this.id}}">{{this.name}}</a>
            </li>
            <div class="divider"></div>
        {{/each}}
    </ul>
</div>